<h1>Log in</h1>

<%= form_tag sessions_path do %>
  <p>
    <%= label_tag :login %><br />
    <%= text_field_tag :login, params[:login] %>
  </p>
  <p>
    <%= label_tag :unhashed_pass %><br />
    <%= password_field_tag :unhashed_pass %>
  </p>
  <p class="button"><%= submit_tag "Log in" %></p>
<% end %>
